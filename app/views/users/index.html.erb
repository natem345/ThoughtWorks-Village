<h1>Listing users</h1>

<table>
  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Location</th>
    <th>Current Position</th>
    <th>School</th>
    <th>Major</th>
    <th>Total Years of Experience</th>
    <th>Email</th>
    <!--<th>Password</th>-->
    <th>Phone</th>
    <th>Interests</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.location %></td>
    <td><%= user.current_position %></td>
    <td><%= user.school %></td>
    <td><%= user.major %></td>
    <td><%= user.total_years_experience %></td>
    <td><%= user.email %></td>
    <!--<td><%= user.password %></td>-->
    <td><%= user.phone %></td>
    <td><%= user.interests %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
